funcion devmode() {
    export PYENV_ROOT="$HOME/.pyenv"
    export PATH="$PYENV_ROOT/bin:$PATH"
    eval "$(pyenv init -)"
    eval "$(pyenv virtualenv-init -)"
    source /usr/share/nvm/init-nvm.sh
}
export PATH=$PATH:~/go/bin
export PATH="$PATH":"$HOME/.pub-cache/bin":"$HOME/.yarn/bin"
export EDITOR=nvim
